    "version": "0.2.0",
    "configurations": [
        {
            "name": "Web",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:8080/call.html?room=TEST&role=admin",
            "webRoot": "${workspaceFolder}/public/js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Server",
            "runtimeArgs": [
                "--import",
                "tsx"
            ],
            "program": "src/backend/main.ts",
            "args": [
                ""
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Speed",
            "runtimeArgs": [
                "--import",
                "tsx"
            ],
            "program": "src/speedtest/main.ts",
            "args": [
                "--admin"
            ]
        }
    ],
}